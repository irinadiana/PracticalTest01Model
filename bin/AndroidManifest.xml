<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ro.pub.cs.systems.eim.practicaltest01Model"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".PracticalTest01ModelMainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PracticalTest01ModelSecondaryActivity"
            android:label="@string/title_activity_practical_test01_model_secondary" >
             <intent-filter>
        		<action android:name="PracticalTest01ModelSecondaryActivity" />
        		<category android:name="android.intent.category.DEFAULT" />
      		</intent-filter>
        </activity>
        <service
      		android:name="PracticalTest01ModelService"
      		android:enabled="true"
      		android:exported="true"/>
    </application>

</manifest>
